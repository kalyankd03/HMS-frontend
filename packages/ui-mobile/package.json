{
  "name": "@hms/ui-mobile",
  "version": "0.0.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "react-native": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "react-native": "./src/index.ts",
      "import": "./dist/index.mjs",
      "module": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --external react --external react-native",
    "dev": "tsup src/index.ts --format cjs,esm --dts --external react --external react-native --watch",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@hms/design-tokens": "workspace:*",
    "react-native-safe-area-context": "^4.10.1",
    "react-native-svg": "^15.2.0",
    "react-native-gesture-handler": "^2.16.2",
    "react-native-reanimated": "^3.8.1",
    "react-native-haptic-feedback": "^2.2.0"
  },
  "devDependencies": {
    "@hms/typescript-config": "workspace:*",
    "@types/react": "^18.3.0",
    "@types/react-native": "^0.73.0",
    "react": "^18.3.0",
    "react-native": "^0.74.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0"
  },
  "peerDependencies": {
    "react": "^18.3.0",
    "react-native": "^0.74.0"
  }
}